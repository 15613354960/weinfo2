/**app.wxss**/
.container {
    font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    color: #666;
    font-size: 28rpx;
}
page {
    background-color: #F2f2f2;
}
.fl{float: left;}
.fr{float: right;}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
